{
  "functions": {
    "api/**/*.php": {
      "runtime": "vercel-php@0.7.0" 
    },
     "admin/**/*.php": {
      "runtime": "vercel-php@0.7.0"
    }
  },
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": ".vercel/output/static" 
      }
    }
  ],
  "routes": [
    { "src": "/api/(.*)", "dest": "/api/$1" },
    { "src": "/admin/(.*)", "dest": "/admin/$1" },
    { "src": "/(.*)", "dest": "/.vercel/output/functions/__render.func" }
  ]
}
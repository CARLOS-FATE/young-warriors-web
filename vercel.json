{
  "version": 2,
  "headers": [
    {
      "source": "/api/(.*).php",
      "headers": [ 
        { "key": "cache-control", "value": "no-store" } 
      ]
    }
  ],
  "routes": [
    { 
      "src": "/api/(.*).php", 
      "dest": "/api/$1.php" 
    },
    
    { 
      "src": "/_astro/(.*)", 
      "headers": { "cache-control": "public, max-age=31536000, immutable" },
      "dest": "/dist/client/_astro/$1" 
    },
    { 
      "src": "/images/(.*)", 
      "headers": { "cache-control": "public, max-age=3600" },
      "dest": "/dist/client/images/$1" 
    },
    { 
      "src": "/favicon.svg", 
      "dest": "/dist/client/favicon.svg" 
    },

    { 
      "handle": "filesystem"
    },
    { 
      "src": "/(.*)", 
      "dest": "/dist/server/entry.mjs" 
    }
  ]
}
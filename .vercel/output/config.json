{
	"version": 3,
	"routes": [
		{
			"handle": "filesystem"
		},
		{
			"src": "^/_astro/(.*)$",
			"headers": {
				"cache-control": "public, max-age=31536000, immutable"
			},
			"continue": true
		},
		{
			"src": "^/_server-islands/([^/]+?)/?$",
			"dest": "_render"
		},
		{
			"src": "^/_image/?$",
			"dest": "_render"
		},
		{
			"src": "^/blog/([^/]+?)/?$",
			"dest": "_render"
		},
		{
			"src": "^/blog/?$",
			"dest": "_render"
		},
		{
			"src": "^/comunidad/?$",
			"dest": "_render"
		},
		{
			"src": "^/entrenadores/?$",
			"dest": "_render"
		},
		{
			"src": "^/jugadores/?$",
			"dest": "_render"
		},
		{
			"src": "^/nosotros/?$",
			"dest": "_render"
		},
		{
			"src": "^/padres/?$",
			"dest": "_render"
		},
		{
			"src": "^/$",
			"dest": "_render"
		}
	]
}